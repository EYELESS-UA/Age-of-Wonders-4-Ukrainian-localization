msgid "TOME_WINDS@SKILL2@NAME"
msgstr "Windborne Scouts"

msgid "TOME_WINDS@SKILL2@PROP_DESCRIPTION"
msgstr "Unit has:[bulletlist][bullet/]A flying mount."

