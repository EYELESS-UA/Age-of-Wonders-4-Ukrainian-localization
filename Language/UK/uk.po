msgstr "Casting Point Upgrade II"

msgid "TOME_GENERIC@CASTING_3@DESCRIPTION"
msgstr ""
"Provides {TacticalPoints}[casttactical/] and {StrategicPoints} "
"[caststrategic/] [abilityblue]Casting Points[/abilityblue]."

msgid "TOME_GENERIC@CASTING_3@NAME"
msgstr "Casting Point Upgrade III"

msgid "TOME_GENERIC@CASTING_4@DESCRIPTION"
msgstr ""
"Provides {TacticalPoints}[casttactical/] and {StrategicPoints} "
"[caststrategic/] [abilityblue]Casting Points[/abilityblue]."

msgid "TOME_GENERIC@CASTING_4@NAME"
msgstr "Casting Point Upgrade IV"

msgid "TOME_GENERIC@CASTING_5@DESCRIPTION"
msgstr ""
"Provides {TacticalPoints}[casttactical/] and {StrategicPoints} "
"[caststrategic/] [abilityblue]Casting Points[/abilityblue]."

msgid "TOME_GENERIC@CASTING_5@NAME"
msgstr "Casting Point Upgrade V"

msgid "TOME_GENERIC@ENCHANTED_CROW_COMPANION@DESCRIPTION"
msgstr ""
"Grants +{ENCHANTED_CROW_STATS_BONUS>BONUS>SightRange} {GLOBAL."
"VisionRangeConcept.Hyperlink} on the {GLOBAL.WorldMapConcept}."

msgid "TOME_GENERIC@ENCHANTED_CROW_COMPANION@NAME"
msgstr "Enchanted Crow Companion"

msgid "TOME_GENERIC@ENCHANTED_CROW_COMPANION@PROPERTY_DESCRIPTION"
msgstr ""
"Unit has:[bulletlist][bullet/]+{ENCHANTED_CROW_STATS_BONUS>BONUS>SightRange} "
"{GLOBAL.VisionRangeConcept.Hyperlink} on the {GLOBAL.WorldMapConcept}."

msgid "TOME_GENERIC@ENCHANTED_EAGLE_COMPANION@DESCRIPTION"
msgstr ""
"[bulletlist][bullet/]+{ENCHANTED_EAGLE_STAT_BONUS>BONUS>SightRange} {GLOBAL."
"VisionRangeConcept.Hyperlink}[bullet/]{GLOBAL.PROPERTIES.TRUE_SIGHT."
"Hyperlink}[/bulletlist]"

msgid "TOME_GENERIC@ENCHANTED_EAGLE_COMPANION@NAME"
msgstr "Enchanted Eagle Companion"

