msgstr "Pioneer^f"

msgid "CULTURE_INDUSTRIOUS@UNIT_02@NAME"
msgstr "Anvil Guard^m"

msgid "CULTURE_INDUSTRIOUS@UNIT_03@NAME"
msgstr "Arbalest^f"

msgid "CULTURE_INDUSTRIOUS@UNIT_04@NAME"
msgstr "Steelshaper^f"

msgid "CULTURE_INDUSTRIOUS@UNIT_05@NAME"
