"[hyperlink,GLOBAL.TurielLoreConcept]{LOC."
"CAMPAIGN@LORDS@LEADER_11@FIRST_NAME} {LOC.CAMPAIGN@LORDS@LEADER_11@LAST_NAME}"
"[/hyperlink]"

