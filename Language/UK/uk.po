msgid "EVENT@TRIBUTES_OF_EVENTCITY@AFFINITY_CHECK@ASTRAL@FAILURE"
msgstr "The {EventStackLeader.Name} prevails, and is furious."

msgid "EVENT@TRIBUTES_OF_EVENTCITY@AFFINITY_CHECK@ASTRAL@HEADER"
msgstr "Let the sky itself battle the dragons in your stead:"

msgid "EVENT@TRIBUTES_OF_EVENTCITY@AFFINITY_CHECK@ASTRAL@SUCCESS"
msgstr "The {EventStackLeader.Name} perishes in the storm."

msgid "EVENT@WHISPERS_OF_WOE@AFFINITY_CHECK@SHADOW@FAILURE"
msgstr "The spirits overwhelm you, barely letting you escape their grasp."

msgid "EVENT@WHISPERS_OF_WOE@AFFINITY_CHECK@SHADOW@HEADER"
msgstr "Attempt to dominate the spirits of the jar:"

msgid "EVENT@WHISPERS_OF_WOE@AFFINITY_CHECK@SHADOW@SUCCESS"
msgstr "Reluctantly they share their knowledge."

msgid "EVENT@WORKED_TO_DEATH@AFFINITY_CHECK@SHADOW@FAILURE"
