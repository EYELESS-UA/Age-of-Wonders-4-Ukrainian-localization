msgid "FORM_TRAITS@SPIDER_MASTERS@EFEFCT_01@DESCRIPTION"
msgstr "{hitpointBonus} <<m:{GLOBAL.HitPointConcept.Hyperlink}>>."

msgid "FORM_TRAITS@SPIDER_MASTERS@EFEFCT_02@DESCRIPTION"
